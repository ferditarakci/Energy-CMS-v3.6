<%SQL = EmptySQL = SQL & "SELECT #___siparis.id AS SipID, #___siparis.tarih AS SipTarih, #___siparis.odeme_tipi, #___siparis.taksit, #___siparis.tutar, #___siparis.durum FROM #___siparis "SQL = SQL & " WHERE #___siparis.uye_id = "& id &" ;"SQL = setQuery( SQL )Set RsObj = data.execute(SQL)If RsObj.Eof Then%><div style="position:absolute;top:10px; right:10px; width:45%;" class="message info block clearfix"><div class="content"><span>Bu üyenin siparişi bulunamadı.</span></div></div></td><%End IfIf Not RsObj.Eof Then%><table class="table_list" style="position:absolute;top:10px; right:10px; width:40%;">	<tr>		<td colspan="7" style="width:5% !important">Siparişler</td>	</tr>	<tr>		<th style="width:5% !important">#</th>		<th style="width:30% !important">Tarih</th>		<th style="width:20% !important">Ödeme&nbsp;Tipi</th>		<th style="width:3% !important">Taksit</th>		<th style="width:15% !important">Tutar</th>		<th style="width:15% !important">Durum</th>		<th style="width:10% !important">Yazdır</th>	</tr><%i = 0Do While not RsObj.eofi = i +1%>	<tr class="<%=TabloiMod(i)%>">		<td><%=RsObj("SipID")%></td>		<td><%=TarihFormatla(RsObj("SipTarih"))%></td>		<td><%=RsObj("odeme_tipi")%></td>		<td><%=RsObj("taksit")%></td>		<td><%=RsObj("tutar")%></td>		<td><span class="<%=styleicon(RsObj("durum"))%>"><%=RsObj("durum")%></span></td>		<td><a class="style-icon-16-print" href="?mod=print&task=siparis&id=<%=RsObj("SipID")%>" onclick="window.open(this.href,'Energy','toolbar=no,location=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=600,height=600');return false;">Yazdır</a></td>	</tr><%RsObj.MoveNext : Loop%></table><%End IfSet RsObj = Nothing%>
